    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautuminen Saamelaisnuorten taidetapahtumaan alkanut</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Almottâttâm Säminuorâi taaidâtábátusân lii álgám</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautuminen Vuotsossa maaliskuun 26. päivänä järjestettävään Saamelaisnuorten taidetapahtumaan on alkanut.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Almottâttâm Säminuorâi taaidâtábátusân, mii uárnejuvvoo Vuáčust njuhčâmáánu 26. peeivi, lii álgám.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtuman teemoina ovat teatteri ja lyhytelokuva.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tábáhtus teeman lii teatter já uánihiselleekove.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautuminen teatterikilpailuun päättyy 2.3. ja elokuvafestivaaliin 13.3.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Almottâttâm teatterkištodmân nohá 2.3. já elleekovefestivaalân 13.3. Tärhibeh tiäđuh almottâtmist täst:</seg>
      </tuv>
    </tu>
