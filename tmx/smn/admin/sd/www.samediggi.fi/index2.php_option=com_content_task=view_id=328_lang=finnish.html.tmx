    <tu>
      <tuv xml:lang="fin">
        <seg>Kutsu arkkitehtuurikilpailun tulosten julkistamistilaisuuteen 4.12.2008</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Povdim arkkitehtkištodem puátusij almostittemtilálâšvuotân</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tervetuloa Saamelaiskulttuurikeskuksen arkkitehtuurikilpailun tulosten julkistamistilaisuuteen, joka pidetään Saamelaismuseo Siidan auditoriossa, Inarissa torstaina 4.12.2008 klo 15.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tiervâpuáttim Säämi kulttuurkuávdáá arkkitehtkištodem puátusij almostittemtilálâšvuotân, mii tuálloo Sämimuseo Siijdâ auditoriost, Anarist tuorâstuv 4.12.2008 tme 15.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tilaisuuden ohjelma</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tilálâšvuođâ ohjelm</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15.00 Tervetuloa</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 15.00 Tiervâpuáttim</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>palkintolautakunnan puheenjohtaja Aulis Kohvakka</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>palhâšumelävdikode saavâjođetteijee Aulis Kohvakka</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kilpailutöiden esittely</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Kištodempargoi čaittâlem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>palkintolautakunnan jäsenet; professori, arkkitehti SAFA Juhani Pallasmaa ja arkkitehti SAFA Anssi Lassila</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>palhâšumelävdikode jesâneh; professor, arkkiteht SAFA Juhani Pallasmaa já arkkiteht SAFA Anssi Lassila</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15.30 Tulosten julkistaminen</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 15.30 Puátusij almostittem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>palkintolautakunnan puheenjohtaja Aulis Kohvakka ja palkintolautakunnan jäsen Juha Guttorm</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>palhâšumelävdikode saavâjođetteijee Aulis Kohvakka já palhâšumelävdikode jeessân Juha Guttorm</seg>
      </tuv>
    </tu>
