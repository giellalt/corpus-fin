    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjien yleinen toimisto on muuttanut</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sämitige almos toimâttâh lii varrim</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjien yleinen toimisto on muuttanut toisiin tiloihin Inarin Riutulaan (Vasatokan kiinteistö).</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sämitige almos toimâttâh lii varrim eres vistijd Aanaar Rivdulân (Vasatokka kiddodâh).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Uusi osoite on Angelintie 696, 99870 Inari.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Uđđâ čujottâs lii Angelintie 696, 99870 Aanaar.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kieli- ja koulutus- ja oppimateriaalitoimistot sijaitsevat Inarin kirkonkylän keskustassa, osoitteessa Lehtolantie 1.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Kielâ- já škovlim- sehe oppâmateriaaltoimâttuvah láá Aanaar markkân kuávdážist, čujottâsâst Lehtolantie 1.</seg>
      </tuv>
    </tu>
