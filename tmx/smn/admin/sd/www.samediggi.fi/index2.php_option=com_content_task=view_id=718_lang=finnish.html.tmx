    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjät toivottaa onnea ja hyvää kesälomaa kaikille !</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sämitigge tuáivut luho já pyeri kesiluámu puohháid !</seg>
      </tuv>
    </tu>
