    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtuman torstain aikataulu muuttuu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Säminuorâi taaiđâtábáhtus tuorâstuv äigitavlu nubbáásm</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Suuren osallistujamäärän vuoksi Saamelaisnuorten taidetapahtuman torstain aikataulu muuttuu.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Stuorrâ uásálisteemere keežild Säminuorâi taaiđâtábáhtus tuorâstuv äigitavlu nubbáásm.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtuma alkaa klo 9:30 ja kestää klo 15:30 asti.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tábáhtus álgá tme 9:30 já pištá tme 15:30 räi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Uuden aikataulun mukainen ohjelma on seuraava</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Uđđâ äigitaavlu miäldásâš ohjelm lii čuávuvâš:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 9:30 Tapahtuma alkaa</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 9:30 Tábáhtus álgá</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 12–13:00 Ruokailu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 12–13:00 Purâdem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 13–14:00 Tapahtuma jatkuu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 13–14:00 Tábáhtus juátkoo</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 14:15–15:00 Vieraileva artisti, Musiikkikeskus</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 14:15–15:00 Artistkyessi, Muusikkuávdáš</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15:00 Tulosten julkistus</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 15:00 Puátusij almostittem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15:30 Tapahtuma päättyy</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>tme 15:30 Tábáhtus nohá</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Taidetapahtumaa voi seurata striimin välityksellä .</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Taaiđâtábáhtus puáhtá keččâđ stream-vuolgâttâssân .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Sajoksessa on myös striimikatsomo, johon tulee ilmoittaa tulostaan etukäteen.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sajosist lii meid streamkejâttâh, moos kalga almottâttâđ muuneeld.</seg>
      </tuv>
    </tu>
