  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjät kehittää kotisivujaan</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sämitigge ovdeed päikkisiijđoidis</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjät on aloittanut kotisivujensa uudistamisen.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Sämitigge lii algâttâm päikkisiijđoidis uđâsmittem.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kotisivuille on lisätty uutta tietoa</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Päikkisijđoid lii lasettum uđđâ tiätu</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>saamelaisista lainsäädännössä</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>sämmilijn lahâaasâtmist</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>, saamelaiskäräjien edustajista eri</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>, sämitige ovdâsteijein jieškote-uv</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>työryhmissä</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>pargojuávhuin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>, alkuperäiskansojen oikeuksia käsittelevistä</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>, algâaalmugij vuoigâdvuođâid kieđâvuššee aalmugijkoskâsijn</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kansainvälisistä sopimuksista</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>sopâmušâin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>sekä alkuperäiskansojen oikeuksien toteutumista valvovien elimien uusimmista</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>sehe algâaalmugij vuoigâdvuođâi olášume koččee orgaanij uđđâsumos</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>suosituksista Suomelle</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>avžuuttâsâin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Uusia sivuosioita käännetään parhaillaan saameksi.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Suomâ staatân. Uđđâ siijđoh jurgâluvvojeh eidu sämikielân.</seg>
      </tuv>
    </tu>
