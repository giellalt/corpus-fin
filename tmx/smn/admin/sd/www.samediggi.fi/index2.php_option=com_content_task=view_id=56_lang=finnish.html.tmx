  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Koulutus- ja oppimateriaalitoimisto</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Škovlim- já oppâmateriaaltoimâttâh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Toimiston vakinaiset työntekijät:</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Toimâttuv fastâpargeeh:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Koulutussihteeri</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Škovlimčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Irmeli Moilanen</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>1.10.-31.12.2015)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- toimiston esimies,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- toimâttuv hovdâ,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- koulutuksen lainsäädännön, hallinnon, rahoituskehityksen, arvioinnin, opetussuunnitelmanormien ja saamelaisopetuksen kehityksen seuranta,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim lahâasâttâs, haldâšem, ruttâdemovdánem, árvuštâllâm, máttááttâsvuávámnoormâi já sämimáttááttâs ovdánem čuávvum,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- koulutus- ja oppimateriaalilautakunnan sihteeri ja esittelijä (koulutuspoliittiset ja yleiset koulutusasiat),</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim- já oppâmateriaallävdikode čällee já oovdânpyehtee (škovlimpoolitlâš já almos škovlimääših),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- saamelaisten koulutusasiain yhteistyöelimen sihteeri ja esittelijä,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- säämi škovlimašij otsâšpargo-orgaan čällee já oovdânpyehtee,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- yhteydenpito saamelaisopetusta järjestäviin kuntiin, valtion opetushallintoon ja pohjoismaisiin sisarelimiin,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- ohtâvuođâtoollâm sämimáttááttâs uárnejeijee kieldáid, staatâ máttááttâshaaldâšmân já tave-eennâmlâš uábbiorgaanáid,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- kansainvälisen kehityksen seuranta.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- aalmugijkoskâsâš ovdánem čuávvum.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Oppimateriaalisihteeri</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Oppâmateriaalčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- koulutus- ja oppimateriaalilautakunnan sihteeri ja esittelijä (oppimateriaaliasiat),</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim- já oppâmateriaallävdikode čällee já oovdânpyehtee (oppâmateriaalääših),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalityön suunnittelu, ohjaus, valvonta ja toimitustyö,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaalpargo vuávám, stivrim, kocceem já toimâttempargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin valmistukseen liittyvä yhteydenpito kouluihin ja opetuksen hallintoon,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal rähtimân lohtâseijee ohtâvuođâtoollâm škovláid já máttááttâs haaldâšmân,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- pohjoismainen yhteistyö.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- tave-eennâmlâš ohtsâšpargo.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin suunnittelu-, toimitus- ja tiedotustyö,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal vuávám-, toimâttem- já tieđettempargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- Saamelaisnuorten taidetapahtuman valmistelu, tiedotus, raportointi ja varainkäyttö,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- Säminuorâi taaidâtábáhtus valmâštâllâm, tieđettem, raportistem já varijkevttim,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin valmistukseen liittyvä yhteydenpito kouluihin ja pohjoismainen yhteistyö,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal rähtimân lohtâseijee ohtâvuođâtoollâm škovláid já tave-eennâmlâš ohtsâšpargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalinäyttelyn ja – varaston ylläpito.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaalčáitálmâs já - vuárhá hoittám.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Toimistosihteeri</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Toimâttâhčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- arkiston ja kirjaston ylläpito,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- arkkâduv já kirjerááju hoittám,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalivarojen käytön seuranta,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaalvarij kevttim čuávvum,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin tiedotustyö, varastointi ja jakelu,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal tieđettempargo, vuorkkim já jyehim,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>- kokousten käytännön järjestelyt.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- čuákkim vuáválâš orniimeh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Oppimateriaalin projektityöntekijät</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Oppâmateriaalpargeeh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Oona Länsman, pohjoissaame</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Oona Länsman, orjâlâškielâ</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Neeta Jääskö, inarinsaame</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Neeta Jääskö, anarâškielâ</seg>
      </tuv>
    </tu>
  </body>
