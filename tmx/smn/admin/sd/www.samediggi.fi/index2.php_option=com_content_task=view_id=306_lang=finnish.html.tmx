    <tu>
      <tuv xml:lang="fin">
        <seg>Hanketoiminta</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Uánihávt</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskulttuurikeskuksen suunnittelun, rakentamisen ja toiminnan käyntiinsaattamisen puolesta toimi kaksi EU-rahoitteista projektia;</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tooimâ vijđesvuotâ 4800 bruttom2</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>a) Saamelaiskulttuurikeskuksen toteuttamisprojekti ja b) Saamelaiskulttuurikeskus Sajoksen liiketoiminta-, markkinointi- ja tuotekehitysprojekti.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Mittomerihadde (alv 0) 12 milj. €</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskulttuurikeskus Sajoksen arkkitehtisuunnittelun takana olivat oululaisen HALO-arkkitehdit Oy:n arkkitehti SAFA Janne Laukka, arkkit.yo Tuomas Niemelä ja arkkit.yo Milla Parkkali.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Tooimah</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Sisustussuunnittelusta vastasi myös Halo-Arkkitehdit Oy.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>haaldâšlâš tooimah</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskulttuurikeskuksen toteuttamishanke</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>kulttuurtooimah</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskulttuurikeskuksen toteuttamishankke toimi 10/2008 - 3/2012.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>máttááttâs- já tutkâmuštoimâ</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Sajos valmistui käyttöön 1.1.2012 ja avautui yleisölle maanantaina 9.1.2012.</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>iäláttâsâi ovdedemtooimah</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Hankkeen laajuus 4800 brm2</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>toorjâpalvâlusah</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tavoitehinta (alv 0) 14,9 milj €</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Siskáldâs</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Eu-tuki 5,0 miljoonaa euroa</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>säämi haldâšem-, kulttuur, škovlim- já mättimčuákki</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Toiminnot</seg>
      </tuv>
      <tuv xml:lang="smn">
