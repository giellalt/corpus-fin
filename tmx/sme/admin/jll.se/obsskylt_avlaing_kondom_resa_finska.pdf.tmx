    <tu>
      <tuv xml:lang="fin">
        <seg>Hyvää matkaa!</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Buorre mátki!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Yhdessä vapaaehtoisjärjestöjen kanssa</seg>
      </tuv>
