    <tu>
      <tuv xml:lang="fin">
        <seg>Milloin tahansa voi kaikkea sattua.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Vaikko goas sáhttá vaikko mii dáhpáhuvvat.</seg>
      </tuv>
