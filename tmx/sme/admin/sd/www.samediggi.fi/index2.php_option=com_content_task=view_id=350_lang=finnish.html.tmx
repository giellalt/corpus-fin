  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tervetuloa Saamelaisten kansallispäivän juhlaan Inariin 6.2.2010 !</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bures boahtin Sámiid álbmotbeaidoaluide Anárii 6.2.2010 !</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjät järjestää Saamelaisten kansallispäivän juhlan Inarin Siida museossa 6.2.2010 klo 17.00-20.00.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámediggi ordne Sámi álbmotbeaivve ávvudoaluid Anáris Siida museas 6.2.2010 dmu 17.00-20.00.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtuma on ulkona revontuliteatterissa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dilálašvuohta lea olgun guovssahasteáhteris.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ohjelmassa on musiikkia, ruokailua ja seurustelua.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Prográmmas lea musihkka, boradeapmi ja servvoštallan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Lapsille järjestetään mm. laskettelua ja poroajelua.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mánáide lágiduvvo ee. čierastallan ja heargevuodjin.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjät järjestää bussikuljetukset Hetasta, Utsjoelta ja Vuotsosta Inariin sekä takaisin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámediggi ordne bussesáhtuid Heahtás, Ohcejogas ja Vuohčus Anárii sihke ruovttoluotta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Bovdehus Kåčč Povdim Kutsu 06.02.2010</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Prográmma - Ohjelma 06.02.2010</seg>
      </tuv>
    </tu>
