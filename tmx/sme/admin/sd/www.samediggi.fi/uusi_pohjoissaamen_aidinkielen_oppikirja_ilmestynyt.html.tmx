    <tu>
      <tuv xml:lang="fin">
        <seg>Uusi pohjoissaamen äidinkielen oppikirja ilmestynyt</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ođđa davvisámegiela eatnigiela oahppogirji almmustuvvan</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Biellocizáš 9 on pohjoissaamen äidinkielen oppikirja peruskoulun 9. luokan oppilaille.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Biellocizáš 9 lea eatnigiela oahppogirji vuođđoskuvlla 9. luohká oahppiide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kirjaan kuuluu myös harjoituskirja.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Girjái gullá maiddái hárjehusgirji.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kirjasarjassa ovat aiemmin ilmestyneet Biellocizáš 7 ja 8 sekä niihin kuuluvat harjoituskirjat.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Girjeráiddus leat almmustuvvan ovdal Biellocizáš 7 ja 8 ja hárjehusgirjjit.</seg>
      </tuv>
    </tu>
    <tu>
