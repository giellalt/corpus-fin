  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten konferenssin ohjelma</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid konfereanssa prográmma</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tervetuloa Saamelaiskäräjien nuorisoneuvoston järjestämään saamelaisnuorten konferenssiin saamelaiskulttuurikeskus Sajokseen Inariin 6.-7.2.2014.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Buresboahtin sámenuoraid konferensii sámekulturguovddáš Sajosii Anárii 6.-7.2.2014.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>10.00 Konferenssin avaus nuorisoneuvoston pj.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konfereanssa lágida Suoma Sámedikki nuoraidráđđi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>10.30 Ei vihapuheelle – liikkeen aktivistit Veeti Nevala ja Seida Sohrabi</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>10.30 Ii vaššiságaide aktivisttat Veeti Nevala ja Seida Sohrabi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>11.00 SDP:n varapuheenjohtaja Eero Vainio: Suvaitsevaisuus ja suvaitsemattomuus Suomessa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>11.00 SDP:a várreságadoalli Eero Vainio: Gierdilvuohta ja ii-gierdilvuohta Suomas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>11.30 Kysely osallistujille</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>11.30 Jearahallan oasseváldiide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>13.00 Työryhmät: Suvaitsevaisuus draaman keinoin</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>13.00 Bargojoavkkut: Gierdilvuohta drámá vehkiin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>14.00 Ryhmien tulosten purku</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>14.00 Joavkobargguid bohtosat</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>16.00 Päivä päättyy</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>16.00 Beaivvi loahpaheapmi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>18.00 Kansallispäivän konsertti Sajoksen auditoriossa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>18.00 Álbmotbeaivvi konsearta, Sajosa auditoriija</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Perjantai 7.2.2014 Parlamenttisali Solju, tulkkaus kaikille Suomessa puhutuille saamenkielille</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bearjadat 7.2.2014 Parlameantasále Solju, tulkon buot Suomas hállojuvvon sámegielaide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>10.00 Paneelikeskustelu saamelaisnuorten identiteetistä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>10.00 Panelaháleštallan sámenuoraid identitehta birra</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Panelisteina:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>34 jahkásaš</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tanja Sanila</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>válmmaštuvvan industriija hábmejeaddjin Lappi universiteahtas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>valmistunut teolliseksi muotoilijaksi Lapin yliopistosta</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>bargá dál sámekultuvrra leanadáiddárin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>toimii saamenkulttuurin läänintaiteilijana</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>eret Čeavetjávrris</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kotoisin Sevettijärveltä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Váhčira bálgosa badjenisu.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>toimii poronhoitajana Vätsärin paliskunnassa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mikkel Näkkäläjärvi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kone- ja tuotantotekniikan opiskelija</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>studere mášen- ja buvttadanteknihka</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Suomen opiskelijakuntien liitto SAMOKin varapuheenjohtaja</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Suoma studeantagotti lihtu SAMOK:a várreságadoalli</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kotoisin Inarista, opiskelee Vaasassa ja työskentelee Helsingissä.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>eret Anáris, studere Vaasas ja bargá Helssegis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Aslak-Uula Länsman</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>27 jahkásaš</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kotoisin Inarista ja asuu parhaillaan Rovaniemellä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>eret Anáris ja orru dál Roavenjárggas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>opiskelee insinööriksi palopäällystön koulutusohjelmassa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>bargan máŋgga sajis sierralágan gádjunsuorggi bargguid</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>toiminut usealla paikkakunnalla eri pelastusalan tehtävissä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dikšon mánáidis ruovttus guhkit áigge</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>ollut myös pitkään lasten kanssa kotona</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>lea nuorabun komponeren rapmusihka suomagillii ja davvisámegillii dáiddárnamain Aziz</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>on tehnyt nuorempana rap-musiikkia suomeksi ja pohjoissaameksi taiteilijanimellä Aziz</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>20 jahkásaš</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>kasvanut poronhoitajaperheessä Rovaniemellä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>bajásšaddan boazodoallobearrašis Roavenjárggas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>äidin puolelta inarinsaamelainen ja äidin kotikylä Nellim on aina ollut tärkeä paikka</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>eatni bealde anáraš ja eatni ruoktogilli Njellim leamaš álo dehálaš báiki</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Opiskelee inarinsaamen ja kulttuurin koulutusohjelmassa SAKK:ssa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>studere anárašgiela ja kultuvrra skuvlenprográmmas SOG:s</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>lukenut pohjoissaamea Kautokeinossa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>studeren davvisámegiela Guovdageainnus.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>13.00 Paneelin yhteenveto ja keskustelua</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>13.00 Panela čoahkkáigeassu ja hálešteapmi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>15.00 Päivä päättyy</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>15.00 Beaivvi loahpaheapmi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ohjelmamuutokset ovat mahdollisia.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Prográmma sáhttá rievdat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tervetuloa !</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Buresboahtin !</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautumiset viimeistään 24.1.2014.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Almmuheamit maŋimustá 24.1.2014.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautumiset ja lisätietoa:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Almmuheamit ja lassidieđut:</seg>
      </tuv>
    </tu>
  </body>
