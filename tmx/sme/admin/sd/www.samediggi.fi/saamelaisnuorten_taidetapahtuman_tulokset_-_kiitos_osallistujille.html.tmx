    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtuman tulokset – kiitos osallistujille</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid dáiddadáhpáhusa bohtosat – giitosat oassálastiide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtuma järjestettiin 14.-15.9.2022 Inarissa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid dáiddadáhpáhus ordnejuvvui 14.-15.9.2022 Anáris.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtumassa nähtiin 17 esitystä kolmella saamen kielellä.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáhpáhusas ledje mielde 17 ovdanbuktima golmma sámegielain.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Järjestäjät kiittävät järjestelyihin osallistuneita, tuomareita ja juontajia sekä kaikkia tapahtumaan osallistuneita.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Lágideaddjit giitet lágidemiide oassálastiid, duopmáriid ja dáhpáhusa láidesteddjiid sihke buohkaid dáhpáhussii oassálastiid.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtumassa oli kolme lajia: musiikki, runous sekä kirjallinen runous ja musiikki.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáhpáhusas ledje golbma šlája: musihkka, dikta sihke girjjálaš barggut musihkas ja divttas.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtumassa oli myös kolme ikäluokkaa: alle 10-vuotiaat, alle 16-vuotiaat ja 16–20-vuotiaat.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáhpáhusas ledje maid golbma ahkeluohkká: vuollel 10-jahkásaččat, vuollel 16-jahkásaččat ja 16-20-jahkásaččat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Jokaisesta lajista ja sarjasta palkittiin tuomariston valitsemat esitykset.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Juohke šlájas ja luohkás bálkkašuvvui duopmáriid válljen ovdanbuktimat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Valitettavasti kirjallisten teosten tuloksia ei pystytty vielä julkistamaan ja tiedotamme tuloksista mahdollisimman pian.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Váidalahtti girjjálašbargguid eat sáhtán vel julggaštit ja mii dieđihit das farggamusat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Palkittujen esitysten tekijät pääsevät sijoituksensa mukaan valitsemaan 50–200 euron palkkion Duodji Shopista tai Siida Shopista.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bálkkašuvvon ovdanbuktimiid dahkkit besset válljet sajáiduvvama mielde 50-200 euro bálkkašumi Duodji Shoppis tai Siida Shoppis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Järjestäjät ovat yhteydessä palkittuihin ensi viikon aikana.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Lágideaddjit leat oktavuođas bálkkašuvvon joavkkuide boahtte vahkku áigge.</seg>
      </tuv>
    </tu>
