  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ravintola Lyeme</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Restoráŋŋa Lyeme</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskulttuurikeskuksen ravintola palvelee kongressien ja kokousten tilausruokailuissa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámekulturguovddáža restoráŋŋa Lyeme bálvala kongreassaid ja čoahkkimiid borademiin.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ravintola sijaitsee keskuksen ensimmäisessä kerroksessa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Restoráŋŋa lea guovddáža vuosttaš gearddis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Sen suurista ikkunoista aukeaa näkymä Juutuanjoelle ja Inarijärvelle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dan stuorra lásain oaidná Juvdujohkii ja Anárjávrái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>AUKIOLOAJAT:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Restoráŋŋas lea sadji 50 olbmui.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ravintola on auki tapahtumien ja kokousten aikaan tilauksesta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Boradanbáikin sáhttá geavahit maid máŋggadoaimmasále ja guovddáža čoahkkinlanjaid.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ravintolassa on asiakastiloja 50 asiakkaalle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Restoráŋŋa Lyeme fitnodatolmmožin doaibmaba Outolintu Oy.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Asiakastilaisuuksissa ruokailutilana voidaan käyttää myös ravintolan vieressä olevaa monitoimisalia sekä keskuksen kokoustiloja.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Oktavuođadieđut:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ravintolan yrittäjänä toimii Outolinty Oy.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>tel. 0400 965 341</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Yhteystiedot:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Šleađgapoasta:</seg>
      </tuv>
    </tu>
