  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Beaivváš Sámi Teahterin näytelmä 24.9.2013</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Beaivváš Sámi Našunálateahtár čájeha 24.9.2013</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Beaivváš Sámi Našunálateáhter esitää "Ridn´oaivi ja nieguid oaidni ("Kuurapää ja unien näkijä") 24.9.2013 Sajoksen Dolla-salissa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Beaivváš Sámi Našunálateáhter: Ridn´oaivi ja nieguid oaidni</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Áillohaš olisi täyttänyt tänä vuonna 70 vuotta ja tässä yhteydessä Beaivváš Teahter näyttää hänen ainoan kirjoittamansa näytelmän.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Áillohaš livččii deavdan dán jagi 70 jagi ja dan oktavuođas Beaivváš Teáhter čájeha su aidna čallon čájálmasa.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Beaivváš kulkee ympäri maailmaa tämän esityksen kanssa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Beaivváš johtá birra máilmmi dáinna čájálmasain.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tiistaina 24.9.2013 he tulevat Inariin, Sajokseen.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Distaga 24.9.2013 sii bohtet Anárii, Sajosii.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Lipunmyynti alkaa tuntia ennen esityksen alkua ovelta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bileahttaid vuovdin álgá diimmu ovdal čájáhusa álgima.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Liput 20€ aikuiset/10€ opiskelijat.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bileahttat 20€ rávesolbmot/10€ studeanttat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ryhmälippu -20%.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Joavkkobileahtta -20%.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Järjestäjä: Beaivváš Sámi Našunálateáhter.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Lágideaddji: Beaivváš Sámi Našunálateáhter.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Miniohjelma myös suomen kielellä</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Miniprográmma</seg>
      </tuv>
    </tu>
