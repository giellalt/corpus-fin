  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtuman torstain aikataulu muuttuu</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid dáiddadáhpáhusa duorastaga áigeplána nuppástuvvá</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Suuren osallistujamäärän vuoksi Saamelaisnuorten taidetapahtuman torstain aikataulu muuttuu.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Stuorra oassálastiid meari dihte Sámenuoraid dáiddadáhpáhusa duorastaga áigeplána nuppástuvvá.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tapahtuma alkaa klo 9:30 ja kestää klo 15:30 asti.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáhpáhus álgá dmu 9:30 ja bistá dmu 15:30 rádjai.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Uuden aikataulun mukainen ohjelma on seuraava</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Prográmma ođđa áigeplána mielde lea</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 9:30 Tapahtuma alkaa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 9:30 Dáhpáhus álgá</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 12–13:00 Ruokailu</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 12–13 Boradeapmi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 13–14:00 Tapahtuma jatkuu</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 13–14:00 Dáhpáhus joatkašuvvá</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 14:15–15:00 Vieraileva artisti, Musiikkikeskus</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 14:15–15:00 Guosseartista, Musihkkaguovddáš</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15:00 Tulosten julkistus</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 15:00 Bohtosiid almmustahttin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>klo 15:30 Tapahtuma päättyy</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>dmu 15:30 Dáhpáhus nohká</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Taidetapahtumaa voi seurata striimin välityksellä .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáiddadáhpáhusa sáhttá čuovvut neahta bokte .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Sajoksessa on myös striimikatsomo, johon tulee ilmoittaa tulostaan etukäteen.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sajosis sáhttá maiddái čuovvut dáhpáhusa neahta bokte, searvamis galgá almmuhit ovddalgihtii.</seg>
      </tuv>
    </tu>
