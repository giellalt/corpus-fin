  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Auditorio Dolla soveltuu konsertti- ja kongressisaliksi.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Auditoria Dolla</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Auditorio Dollassa on nouseva katsomo ja simultaanitulkkausmahdollisuus viidelle kielelle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Auditoria Dolla heive konsearta- ja kongreassasálen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Dolla on pohjossaamea ja tarkoittaa tulta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Auditorias leat buorit čohkkánsajit ja simultánadulkonvejolašvuohta viđa gillii.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Auditorio Dolla on muunneltavissa PERUS-, SEMINAARI- tai MINImuotoon riippuen tilaisuuden luonteesta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Auditoria Dola sáhttá rievdadit VUOĐĐO-, SEMINÁRA- dahje MINIhápmái dilálašvuođa luonddu vuođul.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* 298 m2 (pituus 18,5 m x leveys 17 m)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* 298 m² (guhkkodat18,5 m x govdodat 17 m)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>PERUSMUOTO 260 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>VUOĐĐOHÁPMI 260 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Katsomo: kiinteät istuinpaikat nousevassa katsomossa 254 henkilölle (12 istuinriviä) ja 6 paikkaa luennoitsijoille.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Geahččiidsadji: giddes čohkkánsajit 254 olbmui (12 čohkkánráđa) ja 6 báikki logaldalliide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Näyttämö: 84 m2 (leveys 17 m x syvyys 4,5 m).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Lávdi: 84 m² (govdodat 17 m x čikŋodat 4,5 m).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Soveltuu tilaisuuksiin, joissa tarvitaan keskikokoista näyttämöä ja katsomoa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Heive dilálašvuođaide, main dárbbašit gaskasturrosaš lávddi ja geahččansaji.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ks. pohjakuva oikealla.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gč. vuođđogova olgešbealde.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>SEMINAARIMUOTO 310 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>SEMINÁRAHÁPMI 310 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Katsomo: kiinteät istuinpaikat nousevassa katsomossa 190 henkilölle (9 istuinriviä) + tasalattialla 116 irtotuolia (4 tuoliriviä) ja 4 paikkaa luennoitsijoille.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Geahččiidsadji: giddes čohkkánsajit 190 olbmui (9 čohkkánráđa) + dásseláhtis 116 luovosstuolu (4 stuolloráđa) ja 4 báikki logaldalliide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Istuinpaikkoja yhteensä 310 henkilölle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čohkkánsajit oktiibuot 310 olbmui.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Näyttämö: 43 m2 (leveys 17 m x syvyys 2,5 m).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Lávdi: 43 m² (govdodat 17 m x čikŋodat 2,5 m).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Soveltuu seminaareihin, joissa ei ole tarvetta suurelle näyttämölle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Heive semináraide, main ii leat dárbu stuorra lávdái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ks. pohjakuva.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gč. vuođđogova.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>MINIMUOTO 195 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Máŋggadoaimmasále lea anus sierravárrejumis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Katsomo: kiinteät istuinpaikat 190 henkilölle (9 istuinriviä) ja 5 paikkaa luennnoitsijoille</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Geahččiidsadji: giddes čohkkánsajit 190 olbmui (9 čohkkánráđa) ja 5 báikki logaldalliide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Näyttämö: 136 m2 (leveys 17 m x syvyys 8 m).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Lávdi: 136 m² (govdodat 17 m x čikŋodat 8 m).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ks. pohjakuva.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gč. vuođđogova.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kuvan Monitoimisali on käytössä erillisvarauksesta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gova máŋggadoaimmasále lea anus sierravárrejumis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Dokumenttikamera * Videoneuvotteludekki</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>AUDITORIA DOLA TEKNIHKA JA RUSTTEGAT:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Blueray-soitin, DVD.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Báddenvejolašvuohta</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Sähkökäyttöisen valkokankaan koko: 5 m x 12,5 m * Fläppitaulu</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Elektrovnnalaš vilgesšearpma sturrodat: 5 m x 12,5 m</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>LISÄMAKSUSTA / TILAUKSESTA</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Blohkkatávval</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Tulkkauskoppien koot: 2 kpl x 9,5 m2, 2 kpl x 6 m2 ja 1 kpl x 8 m2</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Bádden- ja simultánadulkonvejolašvuohta viđa gillii (vihtta giddes dulkonlanja), dulkonlanjaid sturrodagat: 2 st x 9,5 m², 2 st x 6 m² ja 1 st x 8 m²</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Tanssimatto näyttämölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Dánsaláhtterátnu lávdái</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Kokoustarjoilut l ue lisää.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Čoahkkinfáladeamit.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>MUUTA HUOMIOITAVAA:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>EARÁ FUOMÁŠANVEARA:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Auditoriossa anniskeluoikeudet.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Auditorias leat addalanvuoigatvuođat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustarjoilut etukäteistilauksesta.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinfálademiid galgá diŋgot ovddalgihtii.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Huoltosisäänkäynti suoraan auditorion näyttämölle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Fuolahussisamannan njuolga auditoria lávdái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Oven leveys 2,2 m.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Uvssa govdodat 2,2 m.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Auditorion takana sijaitseva Monitoimisali Dollagáddi on vuokrattavissa esimerkiksi ruokailuihin tai ryhmätyötilaksi.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>* Auditoria duogábeale Máŋggadoaimmasále Dollagátti sáhttá láigohit ovdamearkka dihte borademiide ja joavkobargosadjin.</seg>
      </tuv>
    </tu>
