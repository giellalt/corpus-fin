    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtuma lähestyy – esityksien ilmoittaminen sekä osallistuminen työpajoihin on nyt auki</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid dáiddadáhpáhus lahkona – dál sáhttá almmuhit loaiddastemiin ja maiddái oassálastimis bargobájiide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuorten taidetapahtumaan voi nyt ilmoittaa esitykset sekä osallistumisen työpajoihin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuoraid dáiddadáhpáhussii sáhttá dál almmuhit iežas loaiddastemiid birra ja maiddái oassálastimis bargobájiide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautumisaika esityksiin ja työpajoihin on 4.-12.4.2023 ja käsityöosioon 4.-19.4.2023 .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Almmuheapmi loaiddastemiide ja bargobájiide lea 4.-12.4.2023 ja duodjeoassái 4.-19.4.2023 .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Ilmoittautuminen tehdään Webropol- ohjelman kautta (linkit alla).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Almmuheapmi bargojuvvo Webropol – prográmma bokte (liŋkkat vuolde).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Tarkat ohjeet esityksiin ja käsityösarjaan löytyvät tapahtuman säännöistä .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dárkilut rávvagat gávdnojit loaiddastemiid ja duodjeoasi birra dáhpáhusa njuolggadusain .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Huomioithan, että osallistuminen esityksiin, käsityöosioon ja työpajoihin toimitetaan eri lomakkeilla .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Amma fuopmášat, ahte oassálastindiehtu loaiddastemiide, duodjeoassái ja bargobájiide doaimmahuvvo sierra skoviin.</seg>
      </tuv>
    </tu>
