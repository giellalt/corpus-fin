    <tu>
      <tuv xml:lang="fin">
        <seg>Tervetuloa pohjoismaiseen saamelaisnuorten konferenssiin!</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bures boahtin davviriikkalaš sámenuoraid konferánsii!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Aika: 26.8.2021 Suomen aikaa klo 9.00-15.00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Áigi: 26.8.2021 Suoma áigge dmu 9.00-15.00</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Paikka: Zoom</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Báiki: Zoom</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssi on maksuton ja avoin kaikille saamelaisnuorille.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánsa lea nuvttá ja rabas buot sámenuoraide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Jaamme ilmoittautuneet työpajoihin ja pyrimme ottamaan toiveet huomioon.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mii juohkit oassálastiid bargobájiide ja viggat váldit vuhtii sávaldagaid.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssissa on simultaanitulkkaus.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánssas lea simultánadulkon.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kaikille osallistujille lähetetään kuvataiteilija Arsi Kevan suunnittelema t-paita.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Buot oassálastiide sáddejuvvo t-báidi, man lea plánen govvadáiddár Arsi Keva.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssin teemana on ilmastonmuutos.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánssa temá lea dálkkádatrievdan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Arktiset alkuperäiskansat kokevat ilmastonmuutoksen rajuimmin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Árktalaš eamiálbmogat vásihit dálkkádatrievdama garrasepmosit.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisnuoret ovat erityisen huolissaan ilmastonmuutoksen vaikutuksesta saamelaisten perinteisiin elinkeinoihin ja saamelaiskulttuuriin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámenuorat leat earenoamáš fuolastuvvan dálkkádatrievdama váikkuhusain sámiid árbevirolaš ealáhusaide ja sámekultuvrii.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssin osana saamelaisnuoret hahmottelevat yhteisen ilmastonmuutoksen vastaisen näkemyksen ja suunnittelevat siihen toimintamalleja.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Oassin konferánssas sámenuorat hábmejit oktasaš dálkkádatrievdama vuostásaš oainnu ja plánejit dasa doaibmamálliid.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaisen parlamentaarisen neuvoston alainen nuorisoneuvosto järjestää kahden vuoden välein pohjoismaisen saamelaisnuorten konferenssin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámi parlamentáralaš ráđi vuollásaš nuoraidráđđi lágida guovtti jagi gaskkaid davviriikkalaš sámenuoraid konferánssa.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssi järjestetään yhteistyönä Norjan, Ruotsin ja Suomen Saamelaiskäräjien nuorisotoimijoiden kanssa.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánsa lágiduvvo ovttasbarggus Norgga, Ruoŧa ja Suoma Sámedikkiid nuoraiddoibmiiguin.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Konferenssin pääjärjestänä toimii Saamelaiskäräjien nuorisoneuvosto.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánssa váldolágideaddji lea Suomabeale Sámedikki nuoraidráđđi.</seg>
      </tuv>
    </tu>
