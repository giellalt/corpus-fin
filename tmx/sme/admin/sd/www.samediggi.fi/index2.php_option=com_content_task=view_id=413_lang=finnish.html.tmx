  <body>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoushuone Násti (isompi)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlatnja Násti</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila Násti (pohjakuvassa Kokoustila 2, isompi tila) soveltuu ryhmätyötilaksi tai pienten kokousten tarpeisiin.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlatnja Násti (bodnegovas čoahkkinsadji 2, stuorát latnja) heive joavkobargui dahje unna čoahkkimiidda.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila Násti sijaitsee keskuksen ensimmäisessä kerroksessa, kokousaulan yhteydessä.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlatnja Násti lea vuosttaš gearddis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila Nástin suuresta lasiseinästä on upea näkymä Juutuanjoelle ja Inarijärvelle.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dan stuorra láseseainnis lea fiinna oainnus Juvdujohkii ja Anárjávrái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila Násti on yhdistettävissä pienempään, 10 hengen kokoustila Silbaan, jolloin saadaan tilaa 25 henkilölle (50 m2).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlanja Nástti lea vejolaš ovttastahttit unnit, 10 olbmo čoahkkinlanjain Silbbain, nu ahte šaddá sadji oktiibuot 25 olbmui (50m2).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Násti on pohjoissaamea ja tarkoittaa tähteä.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>STURRODAT:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>TILAVUUS:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Viidodat 30m2 (s. 5,5m x 5,5m)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Pinta-ala: 30 m2 (n. 5,5 m x 5,5 m)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>KAPASITEAHTTA:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>KAPASITEETTI:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Luohkkáhámis: 15 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Luokkamuodossa: 15 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Diplomáhtahámis: 15 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Diplomaattimuodossa: 15 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>U-hámis: 15 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>U-muodossa: 15 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Teáhterhámis: 20 olbmui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Teatterimuodossa: 20 henkilölle</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>TEKNIHKA JA RUSTTEGAT</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>TEKNIIKKA JA VARUSTEET</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dataprojektor 2500, 16:9</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Dataprojektori 2500, 16:9</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dokumeantaprojektor</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Dokumenttiprojektori</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Blu-ray-čuojanas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Blueray-soitin</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>WLAN</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* WLAN</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Blohkkatávval</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Fläppitaulu</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gálvvut: Čoahkkinbeavddit ja -stuolut 15 olbmui, minigievkkan (keramihkkauvdna, čáhci, lihttebassanmašiidna, jiekŋaskábe).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Kalusteet: kokouspöydät ja kokoustuolit 15 henkilölle, minikeittiövarustus (keraaminen liesi, vesipiste, astianpesukone, jääkaappi).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>LASSEMÁVSSUS/ DIŊGOJUMIS:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>ERILLISTILAUKSESTA/LISÄMAKSUSTA:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinfáladeamit.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>* Kokoustarjoilut l ue lisää.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>EARÁ FUOMÁŠANVEARA:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>MUUTA HUOMIOITAVAA:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinsajis leat addalanvuoigatvuođat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustilassa anniskeluoikeudet.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bodnegovas vuolábealde čoahkkinlatja 2=Násti.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Alla olevassa pohjakuvassa Kokoustila 2=Násti.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlatnja 1=Silba.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila 1=Silba.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinlanjat 1+2=Silbanásti.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Kokoustila 1+2 yhdistettynä=Silbanásti.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkinsadjái lea vejolaš diŋgot borramušbálvalusaid ovddalgihtii.</seg>
      </tuv>
    </tu>
