      <tuv xml:lang="fin">
        <seg>Uusi nuorisoneuvosto on asetettu Uumajassa</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ođđa nuoraidráđđi lea ásahuvvon Upmis</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Saamelaiskäräjien kokous asetti nuorisoneuvoston toimikaudeksi 2014-2015 kokouksessaan Uumajassa 19.2.2014.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámedikki čoahkkin ásahii nuoraidráđi doaibmaáigodahkii 2014-2015 čoahkkimisttis Upmis 19.2.2014.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Nuorisoneuvostoa edusti kokouksessa Niila Rahko.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Nuoraidráđi lei ovttasteamen čoahkkimis Niila Rahko.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Nuorisoneuvosto 2014-2015</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Nuoraidráđđi 2014-2015</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Jäsenet:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Lahttut:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="fin">
        <seg>Niila Rahko, puheenjohtaja - varajäsen Mikkel Näkkäläjärvi</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Niila Rahko, ságadoalli - várrelahtu Mikkel Näkkäläjärvi</seg>
      </tuv>
